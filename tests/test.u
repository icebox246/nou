export add;
export add2;
export echo;
export big;
export nop;
export variables;
export expressions;
export expressions2;
export functions;

add := fn a: i32, b: i32 -> i32 {
    return a + b;
};

add2 := fn x: i32 -> i32 {
    return x + 2;
};

echo := fn x: i32 -> i32 {
    return x;
};

big := fn -> i32 {
    return 123456789;
};

nop := fn {};

variables := fn -> i32 {
    b := i32 21;
    a := i32 b + b; 
    return a;
};

expressions := fn -> i32 {
    a := i32 43 - 3 + 2; 
    return a;
};

expressions2 := fn -> i32 {
    a := i32 2 * (20 + 1); 
    return a;
};

functions := fn -> i32 {
    a := i32 add(20, 21);
    f := fn {
        t := i32;
        t = 42;
    };
    f();
    return a;
};
